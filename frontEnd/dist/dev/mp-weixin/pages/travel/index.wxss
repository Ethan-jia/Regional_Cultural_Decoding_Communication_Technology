.travel-wrap .privacy-wrap {
  background-color: rgb(243, 243, 243);
  width: 60vw;
  height: 35vh;
  display: flex;
  flex-direction: column;
  align-items: center;
  border-radius: 3%;
}
.travel-wrap .privacy-wrap .privacy-title {
  font-size: 18px;
  font-weight: bold;
  letter-spacing: 1px;
  margin-top: 2vh;
}
